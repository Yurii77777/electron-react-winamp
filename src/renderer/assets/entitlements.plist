<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Disable sandbox if you need access to the file system -->
    <key>com.apple.security.app-sandbox</key>
    <false/>

    <key>com.apple.security.cs.allow-jit</key>
    <true/>

    <key>com.apple.security.cs.allow-unsigned-executable-memory</key>
    <true/>

    <key>com.apple.security.cs.debugger</key>
    <true/>

    <!-- Permission to read/write files selected by the user -->
    <key>com.apple.security.files.user-selected.read-write</key>
    <true/>

    <!-- Permission to access files in public directories -->
    <key>com.apple.security.files.downloads.read-write</key>
    <true/>
    <key>com.apple.security.files.documents.read-write</key>
    <true/>

    <!-- Permission for temporary files -->
    <key>com.apple.security.files.bookmarks.app-scope</key>
    <true/>

    <!-- Network connections (if necessary) -->
    <key>com.apple.security.network.client</key>
    <true/>
</dict>
</plist>
